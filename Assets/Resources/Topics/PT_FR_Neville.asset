      playerIsTalking: 0
      newTopic: {fileID: 0}
      newEvidence: {fileID: 0}
      newSuspect: {fileID: 0}
  accuseCop:
    choiceResponse:
    - text: A lot of bad seeds in the Orba program, I remember them well. Are you
