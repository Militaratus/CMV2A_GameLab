      playerIsTalking: 1
      newTopic: {fileID: 0}
      newEvidence: {fileID: 0}
      newSuspect: {fileID: 0}
  badCop:
    choiceResponse:
    - text: 'Think harder, Neville. A man''s dead over there - tell me exactly what
